{
  "_version": "1.59.0",
  "sap.app": {
    "id": "cart",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "SAP ssarum Mall",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.12.1",
      "toolsId": "1fd58b5a-026b-4816-a4be-b2188b0e907e"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/SAP/ZGWSAPSSARUM_ZBB_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.124.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "cart.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "cart.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":?query:",
          "target": [
            "TargetMain"
          ]
        },
        {
          "name": "RouteCart",
          "pattern": "Cart/{key1}", 
          "target": [
              "TargetCart"
          ]
      },
      {
        "name": "RouteLogin",
        "pattern": "Login/:key1:/:key2:", 
        "target": [
            "TargetLogin"
        ]
    },
    {
      "name": "RouteSignup",
      "pattern": "Signup/{key1}", 
      "target": [
          "TargetSignup"
      ]
  }
      ],
      "targets": {
        "TargetMain": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Main",
          "viewName": "Main"
        },
        "TargetCart": {
          "viewType": "XML",
          "transition": "fade",
          "clearControlAggregation": false,
          "viewId": "Cart",
          "viewName": "Cart"
      },
      "TargetLogin": {
        "viewType": "XML",
        "transition": "fade",
        "clearControlAggregation": false,
        "viewId": "Login",
        "viewName": "Login"
    },
    "TargetSignup": {
      "viewType": "XML",
      "transition": "fade",
      "clearControlAggregation": false,
      "viewId": "Signup",
      "viewName": "Signup"
  }
      }
    },
    "rootView": {
      "viewName": "cart.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
